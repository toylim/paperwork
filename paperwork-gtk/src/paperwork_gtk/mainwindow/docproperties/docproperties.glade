<?xml version="1.0" encoding="utf-8"?>
<interface domain="paperwork_gtk">
  <requires lib="gtk+" version="3.20"/>

  <object class="HdyHeaderBar" id="docproperties_header">
    <property name="visible">True</property>
    <property name="title" translatable="yes">Properties</property>
    <property name="subtitle"></property>
    <property name="show_close_button">False</property>
    <property name="decoration_layout"></property>

    <child>
      <object class="GtkButton" id="docproperties_cancel">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="receives_default">True</property>
        <property name="use_stock">True</property>
        <property name="label">gtk-cancel</property>
      </object>
      <packing>
        <property name="pack_type">start</property>
      </packing>
    </child>

    <child>
      <object class="GtkButton" id="docproperties_back">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="use_action_appearance">False</property>
        <property name="use_underline">True</property>
        <property name="use_stock">True</property>
        <property name="label">gtk-apply</property>
      </object>
      <packing>
        <property name="pack_type">end</property>
      </packing>
    </child>
  </object>

  <object class="GtkScrolledWindow" id="docproperties_body">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkBox" id="docproperties_box">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <property name="name">docproperties_body</property>
      </object>
    </child>
  </object>
</interface>
