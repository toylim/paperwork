<?xml version="1.0" encoding="utf-8"?>
<interface>
  requires lib="gtk+" version="3.20"/>
  <object class="GtkBox" id="scan_buttons">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkButton" id="scan_button">
        <property name="use_action_appearance">False</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="label" translatable="yes">Scan</property>
        <style>
          <class name="scan-button-left"/>
        </style>
      </object>
      <packing>
        <property name="position">0</property>
      </packing>
    </child>
    <child>
      <object class="GtkMenuButton" id="scan_switch">
        <property name="use_action_appearance">False</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <!-- <property name="menu_model">scan_menu</property> -->
        <style>
          <class name="scan-button-right"/>
        </style>
      </object>
      <packing>
        <property name="position">1</property>
      </packing>
    </child>
  </object>
</interface>
